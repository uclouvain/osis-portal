msgid ""
"You are not registered for any course for the %(annee_academique)s academic "
"year."
msgstr ""
"Vous n'êtes inscrit.e à aucune formation pour l'année académique "
"%(annee_academique)s."

#, python-brace-format
msgid ""
"You are not registered for any course for the {annee_academique} academic "
"year."
msgstr ""
"Vous n'êtes inscrit.e à aucune formation pour l'année académique "
"{annee_academique}."

#, python-format
msgid ""
"You are not registered for any of the %(nombre_evaluation_organisee)s "
"evaluation organized for the January session."
msgstr ""
"Vous n'êtes inscrit.e à aucune évaluation sur le(s) "
"%(nombre_evaluation_organisee)s évaluation(s) organisée(s) à la session de "
"janvier."

#, python-format
msgid ""
"You are not registered for any of the %(nombre_evaluation_organisee)s "
"evaluation organized for the June session."
msgstr ""
"Vous n'êtes inscrit.e à aucune évaluation sur le(s) "
"%(nombre_evaluation_organisee)s évaluation(s) organisée(s) à la session de "
"juin."

#, python-format
msgid ""
"You are not registered for any of the %(nombre_evaluation_organisee)s "
"evaluation organized for the September session."
msgstr ""
"Vous n'êtes inscrit.e à aucune évaluation sur le(s) "
"%(nombre_evaluation_organisee)s évaluation(s) organisée(s) à la session de "
"septembre."

#, python-format
msgid ""
"You are registered for %(nombre_inscription)s of the "
"%(nombre_evaluation_organisee)s evaluation organized for the January session."
